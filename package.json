{
  "name": "homepage",
  "license": "MIT",
  "version": "0.0.0",
  "scripts": {
    "clean-pub": "rm -rf ./dist/assets",
    "dev": "vite",
    "esbuild": "esbuild --bundle src/main.tsx '--define:process.env.NODE_ENV=\"development\"' --target=es2016 --outdir=dist/assets --sourcemap",
    "type-check": "tsc --noEmit",
    "build": "run-s clean-pub type-check esbuild",
    "gh-pages": "gh-pages -d dist",
    "deploy": "run-s build gh-pages"
  },
  "dependencies": {
    "@pika/react": "^16.13.1",
    "@pika/react-dom": "^16.13.1",
    "redux": "^4.0.5",
    "styled-components": "^5.1.1",
    "typescript": "^3.9.7"
  },
  "devDependencies": {
    "@types/jest": "^26.0.9",
    "@types/node": "^14.0.27",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "@types/styled-components": "^5.1.2",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "esbuild": "^0.6.22",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-standard": "^4.0.1",
    "gh-pages": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "rollup": "^2.25.0",
    "typescript-styled-plugin": "^0.15.0"
  }
}
